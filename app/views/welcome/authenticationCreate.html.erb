<h1>Widgets</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Email</th>
      <th>Fname</th>
      <th>Lname</th>
      <th>Dob</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @widgets.each do |widget| %>
      <tr>
        <td><%= widget.email %></td>
        <td><%= widget.fName %></td>
        <td><%= widget.lname %></td>
        <td><%= widget.dob %></td>
        <td><%= link_to 'Show', widget %></td>
        <td><%= link_to 'Edit', edit_widget_path(widget) %></td>
        <td><%= link_to 'Destroy', widget, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


